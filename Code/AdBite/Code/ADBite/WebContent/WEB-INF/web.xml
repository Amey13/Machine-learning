<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>ADBite</display-name>
	<filter>
		<description>
		</description>
		<display-name>ADBiteFilter</display-name>
		<filter-name>ADBiteFilter</filter-name>
		<filter-class>filter.ADBiteFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>ADBiteFilter</filter-name>
		<url-pattern>*</url-pattern>
	</filter-mapping>
	<listener>
		<listener-class>filter.ADBiteSessionListener</listener-class>
	</listener>
	<listener>
		<listener-class>filter.ADBiteListener</listener-class>
	</listener>
	<servlet>
		<description>
		</description>
		<display-name>GetDataFromUrl</display-name>
		<servlet-name>GetDataFromUrl</servlet-name>
		<servlet-class>
		servlet.GetDataFromUrl</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>Training</display-name>
		<servlet-name>Training</servlet-name>
		<servlet-class>
		servlet.Training</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>GetDataFromUrl</servlet-name>
		<url-pattern>/GetDataFromUrl</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Training</servlet-name>
		<url-pattern>/Training</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<error-page>
		<error-code>404</error-code>
		<location>/404.jsp</location>
	</error-page>
</web-app>
